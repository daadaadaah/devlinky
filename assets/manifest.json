  
{
  "manifest_version": 2,
  "name": "develinky",
  "description": "This is web clipper for devlink website",
  "version": "1.0.0",
  "icons": {
    "128": "icon.png"
  },
  "browser_action": {
    "default_title": "develinky",
    "default_popup": "index.html"
  },
  "permissions": [
    "<all_urls>",
    "tabs"
  ]
}
